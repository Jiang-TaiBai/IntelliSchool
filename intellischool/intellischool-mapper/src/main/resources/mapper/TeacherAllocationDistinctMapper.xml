<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dajiangsai.mapper.TeacherAllocationDistinctMapper">
  
  <select id="queryTeacherAllocationDistinct" parameterType="Map" resultType="com.dajiangsai.pojo.vo.TeacherAllocationDistinctVO">
    SELECT DISTINCT
      grade_name,
      sub_name,
      id
    FROM
      teacher
  </select>
</mapper>